<view class="container"><view>{{"用户名："+userinfo.username}}</view><view>{{"年龄："+userinfo.age}}</view><view>{{"邮箱："+userinfo.email}}</view><block s-if="{{userinfo.username}}"><button style="margin-top:30rpx;" type="warn" data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" bindtap="__e">退出登录</button></block><block s-else><button style="margin-top:30rpx;" type="primary" data-event-opts="{{[['tap',[['getUserinfo',['$event']]]]]}}" bindtap="__e">获取用户信息</button></block></view>